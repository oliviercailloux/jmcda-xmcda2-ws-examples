<?xml version="1.0" encoding="UTF-8"?>
<program_description
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://www.decision-deck.org/ws/_downloads/description.xsd">
	<program name="CsvImport" provider="J-MCDA" version="${version}"
		displayName="CsvImport" />
	<documentation>
		<description>Transforms a performance table from a comma-separated values (CSV) file to XMCDA compliant files.</description>
		<contact>Olivier Cailloux &lt;olivier.cailloux@dauphine.fr></contact>
		<url>https://github.com/oliviercailloux/jmcda-xmcda2-ws-examples</url>
	</documentation>
	<parameters>
		<input displayName="performances" name="performances" id="input0"
			isoptional="0">
			<documentation>
				<description>The alternatives, criteria, performances as a CSV file.</description>
			</documentation>
			<!-- <xmcda tag="alternatives" /> -->
		</input>
		<output displayName="alternatives" name="alternatives" id="output0">
			<documentation>
				<description>The equivalent alternatives.</description>
			</documentation>
			<xmcda tag="alternatives" />
		</output>
		<output displayName="criteria" name="criteria" id="output1">
			<documentation>
				<description>The equivalent criteria.
				</description>
			</documentation>
			<xmcda tag="criteria" />
		</output>
		<output displayName="performances" name="performances" id="output2">
			<documentation>
				<description>The equivalent performances.
				</description>
			</documentation>
			<xmcda tag="performanceTable" />
		</output>
		<output displayName="messages" name="messages" id="output3">
			<documentation>
				<description>A status message.</description>
			</documentation>
			<xmcda tag="methodMessages" />
		</output>
	</parameters>
</program_description>